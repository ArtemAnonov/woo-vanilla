{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "attach",
      "name": "F",
      "port": 9222,
      "enableContentValidation": false,
      "webRoot": "${workspaceFolder}/",
      // "urlFilter": "http://localhost:*",
